{
  "extends": "airbnb-base",
  "rules": {
    "arrow-parens": "off",
    "arrow-body-style": "off",
    "strict": "off",
    "comma-dangle": "off",
    "eol-last": "off",
    "no-underscore-dangle": "off",
    "max-len": ["error", 150, { "ignoreUrls": true }],
    "consistent-return": "off",
    "import/no-extraneous-dependencies": "off",
    "no-param-reassign": "off",
    "no-shadow": "off"
  }
}